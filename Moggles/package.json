{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.30",
    "aspnet-webpack": "^2.0.3",
    "autoprefixer": "^9.7.3",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-vue": "^5.2.3",
    "expect": "^24.9.0",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flush-promises": "^1.0.2",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^1.0.0",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "pretty-checkbox-vue": "^1.1.9",
    "sass-loader": "^6.0.7",
    "sinon": "^4.5.0",
    "style-loader": "^0.20.3",
    "url-loader": "^0.6.2",
    "vue-jest": "^2.6.0",
    "vue-loader": "^14.2.4",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^3.12.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack",
    "dev-full": "cross-env NODE_ENV=development webpack --config webpack.config.vendor.js && cross-env NODE_ENV=development webpack",
    "stage": "cross-env webpack --env.staging",
    "stage-full": "cross-env webpack --config webpack.config.vendor.js --env.staging && cross-env webpack --env.staging",
    "prod": "cross-env webpack --env.prod",
    "prod-full": "cross-env webpack --config webpack.config.vendor.js --env.prod && cross-env webpack --env.prod",
    "test-full": "cross-env NODE_ENV=development webpack --config webpack.config.vendor.js && cross-env mocha-webpack --webpack-config webpack.config.js --webpack-env test --require ClientApp/tests/setup.js ClientApp/tests/**/*.spec.js",
    "test": "cross-env mocha-webpack --webpack-config webpack.config.js --webpack-env test --require ClientApp/tests/setup.js ClientApp/tests/**/*.spec.js",
    "test-j": "jest",
    "lint": "eslint ClientApp/**/*.{js,vue}",
    "lint-fix": "eslint ClientApp/**/*.{js,vue} --fix"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bootstrap-sass": "^3.4.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "popper.js": "^1.16.0",
    "uiv": "^0.32.3",
    "vue": "^2.6.10",
    "vue-good-table": "^2.18.1",
    "vue-js-modal": "^1.3.31",
    "vue-moment": "^4.0.0",
    "vue-strap": "github:wffranco/vue-strap"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/ClientApp/tests/",
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
